<%= render :partial => 'colibri/admin/shared/product_sub_menu' %>

<%= render :partial => 'colibri/shared/error_messages', :locals => { :target => @prototype } %>

<%= form_for [:admin, @prototype] do |f| %>
<fieldset data-hook="new_prototype">
  <legend align="center"><%= Colibri.t(:new_prototype) %></legend>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= render :partial => 'colibri/admin/shared/new_resource_links' %>
</fieldset>
<% end %>
