<%= render :partial => 'colibri/admin/shared/product_sub_menu' %>

<% content_for :page_title do %>
  <%= Colibri.t(:editing_prototype) %>
<% end %>

<% content_for :page_actions do %>
  <li>
    <%= button_link_to Colibri.t(:back_to_prototypes_list), colibri.admin_prototypes_path, :icon => 'icon-arrow-left' %>
  </li>
<% end %>

<%= render :partial => 'colibri/shared/error_messages', :locals => { :target => @prototype } %>

<%= form_for [:admin, @prototype] do |f| %>
  <fieldset class="no-border-top">
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'colibri/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
